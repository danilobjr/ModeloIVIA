<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300|Raleway:100" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/Plugins/DataTables/css/demo_table_jui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/Plugins/DataTables_ColReorder/css/ColReorder.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/Plugins/DataTables_ColVis/css/ColVis.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/Plugins/DataTables_TableTools/css/TableTools.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/Plugins/DataTables/jquery.dataTables.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Plugins/DataTables_ColReorder/ColReorder.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Plugins/DataTables_ColVis/ColVis.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Plugins/DataTables_TableTools/TableTools.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Plugins/ezSlider/jquery.ezSlider.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/App/Main/Namespace.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Main/MainViewModel.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Main/MainScript.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/App/Servidor/Servidor.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Componentes/DataTables.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Componentes/Slider.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Componentes/Validador.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Componentes/Botao.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/Componentes/Dialog.js")" type="text/javascript"></script>

    @RenderSection("ScriptsOnHead", false)
    
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1><a href="/Home">IVIA</a></h1>
            </div>
            @*<div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>*@
            <nav>
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("DataTables", "Index", "Usuario")</li>
                    <li>@Html.ActionLink("CRUD", "Index", "UsuarioJS")</li>
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
            <span>
                Disigned by: <a href="http://twitter.com/danilojrr" target="_blank">@@DaniloJrr</a>
            </span>
        </footer>
    </div>

    @Html.Partial("Loader")
</body>
</html>
